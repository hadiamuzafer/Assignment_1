//Q17
//step1
/*Start with your program from Exercise 16. Add a new line that prints a message saying
that you can invite only two people for dinner.*/
let guestlist: Array<string>=[
    "fatima","mahreen","iqra","malika","mehak","minahil"
]

console.log(`${guestlist} i can invite only two people for dinner`)
//step2
/* Remove guests from your list one at a time until only two names remain in your list.
Each time you pop a name from your list, print a message to that person letting them
know you’re sorry you can’t invite them to dinner*/
let removename1=guestlist.pop()
console.log(`${removename1} sorry i can't invite you for a dinner ` );

let removename2=guestlist.pop()
console.log(`${removename2} sorry i can't invite you for a dinner ` );

let removename3=guestlist.pop()
console.log(`${removename3} sorry i can't invite you for a dinner ` );

let removename4=guestlist.pop()
console.log(`${removename4} sorry i can't invite you for a dinner ` );
//step3

/* Print a message to each of the two people still on your list, letting them know they’re
still invited.*/
let guestlist2:Array<string>=[
    "fatima" ,"mahreen"
]
console.log(`${ guestlist2} both of you still invited for a dinner tonight)
//step4
/*• Remove the last two names from your list, so you have an empty list. Print your list to
make sure you actually have an empty list at the end of your program.*/
let removename5=guestlist.pop()
console.log(`${removename5} sorry i can't invite you for a dinner ` );
let removename6=guestlist.pop()
console.log(`${removename6} sorry i can't invite you for a dinner ` );

console.log(`${guestlist}`)
